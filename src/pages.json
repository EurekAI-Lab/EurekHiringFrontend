{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "AI面试项目",
    "navigationBarBackgroundColor": "#f8f8f8",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#018d71",
    "backgroundColor": "#F8F8F8",
    "borderStyle": "black",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [
      {
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/homeHL.png",
        "pagePath": "pages/question/question",
        "text": "首页"
      },
      {
        "iconPath": "static/tabbar/example.png",
        "selectedIconPath": "static/tabbar/exampleHL.png",
        "pagePath": "pages/about/about",
        "text": "面试待处理列表"
      },
      {
        "iconPath": "static/tabbar/example.png",
        "selectedIconPath": "static/tabbar/exampleHL.png",
        "pagePath": "pages/about/mspj",
        "text": "组件集合"
      },
      {
        "iconPath": "static/tabbar/example.png",
        "selectedIconPath": "static/tabbar/exampleHL.png",
        "pagePath": "pages/test/index",
        "text": "视频上传"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/question/question",
      "type": "home",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/about/about",
      "type": "page",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/about/mspj",
      "type": "page"
    },
    {
      "path": "pages/question/add-question",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "增加面试题目"
      }
    },
    {
      "path": "pages/question/edit-question",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "编辑面试题目"
      }
    },
    {
      "path": "pages/test/index",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "测试视频上传"
      }
    }
  ],
  "subPackages": []
}