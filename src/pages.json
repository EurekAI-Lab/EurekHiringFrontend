{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "AI面试项目",
    "navigationBarBackgroundColor": "#f8f8f8",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#018d71",
    "backgroundColor": "#F8F8F8",
    "borderStyle": "black",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px"
  },
  "pages": [
    {
      "path": "pages/question/question",
      "type": "home",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/about/about",
      "type": "page",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/about/mspj-loading",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "面试报告生成中",
        "app-plus": {
          "bounce": "none"
        }
      }
    },
    {
      "path": "pages/about/mspj",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "面试报告",
        "enablePullDownRefresh": true,
        "app-plus": {
          "bounce": "none"
        }
      }
    },
    {
      "path": "pages/camera/index",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "视频面试"
      }
    },
    {
      "path": "pages/interviews/record-simulate",
      "type": "page",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/interviews/record",
      "type": "page",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/question/add-question",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "增加面试题目"
      }
    },
    {
      "path": "pages/question/edit-question",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "编辑面试题目"
      }
    },
    {
      "path": "pages/test/index",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "环境测试页面"
      }
    }
  ],
  "subPackages": []
}